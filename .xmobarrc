Config { font = "xft:InconsolataForPowerline Nerd Font Medium:size=9:antialias=true",
    bgColor = "#000000",
    fgColor = "#008000",
    position = Static { xpos = 0, ypos = 0, width = 3840, height = 26 },
    lowerOnStart = False,
    commands = [
        Run Weather "KLHM" ["-t","Lincoln <tempF>째","-L","60","-H","85","--normal","#008000" , "--high","red","--low","lightblue"] 36000,
        Run Weather "KSFO" ["-t","Sanfran <tempF>째","-L","60","-H","85","--normal","#008000" , "--high","red","--low","lightblue"] 36000,
        Run MultiCpu ["-t","cpu <total0> <total1> <total2> <total3>","-L","30","-H","60","-h","red","-l","#008000","-n","#FFFFCC"] 10,
        Run CoreTemp ["-t", "temp <core0>째<core1>째", "-L", "40", "-H", "60", "-l", "#008000", "-n", "#008000", "-h", "red"] 50,
        Run Memory ["-t","mem <usedratio>%","-H","15000","-L","8192","-h","#FFB6B0","-l","#008000","-n","#FFFFCC"] 10,
        Run Battery ["-t", "bat <left>%", "-L", "20", "-H", "75", "-h", "#008000", "-n", "yellow", "-l", "red"] 60,
        Run DynNetwork [] 10,
        Run Date "%m/%d %l:%M:%S" "date" 10,
        Run StdinReader
    ],
    sepChar = "%",
    alignSep = "}{",
    template = "%StdinReader% }{ %multicpu%   %coretemp%   %memory%   %battery%   %KLHM%   %KSFO%  <fc=#008000>%date%</fc>"
}
